    <command:details>
      <command:name>Invoke-WebSearch</command:name>
      <command:verb>Invoke</command:verb>
      <command:noun>WebSearch</command:noun>
      <maml:description>
        <maml:para>Do a quick search online</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>Search anything using online search engine (Default: Google)</maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Invoke-WebSearch</maml:name>
        <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="True (ByValue)" position="2" aliases="none">
          <maml:name>SearchQuery</maml:name>
          <maml:Description>
            <maml:para>Search Querry to be searched for</maml:para>
          </maml:Description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>None</dev:defaultValue>
        </command:parameter>
        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="3" aliases="none">
          <maml:name>SearchEngine</maml:name>
          <maml:Description>
            <maml:para>Name of Search Engine to be used</maml:para>
          </maml:Description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
          <dev:type>
            <maml:name>String</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>Google</dev:defaultValue>
        </command:parameter>
      </command:syntaxItem>
    </command:syntax>
    <command:parameters>
      <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="True (ByValue)" position="2" aliases="none">
