          <maml:name>System.String</maml:name>
        </dev:type>
        <maml:description>
          <maml:para>The search query</maml:para>
        </maml:description>
      </command:inputType>
    </command:inputTypes>
    <command:returnValues>
      <command:returnValue>
        <dev:type>
          <maml:name>None</maml:name>
        </dev:type>
